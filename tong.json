{
  "mcpServers": {
    "filesystem": {
      "timeout": 60,
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "."
      ],
      "transportType": "stdio"
    },
    "git": {
      "timeout": 300,
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ],
      "transportType": "stdio"
    }
  },
  "masterLLM": {
    "type": "deepseek",
    "params": {}
  },
  "embeddingLLM": {
    "type": "deepseek",
    "params": {}
  }
}